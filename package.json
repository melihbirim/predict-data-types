    "url": "git+https://github.com/melihbirim/predict-data-types.git"
  },
  "bugs": {
    "url": "https://github.com/melihbirim/predict-data-types/issues"
  },
  "homepage": "https://github.com/melihbirim/predict-data-types#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
